{
  "name": "angular-components",
  "version": "0.10.12",
  "author": {
    "name": "UiPath Inc",
    "url": "https://uipath.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/UiPath/angular-components.git"
  },
  "license": "MIT",
  "keywords": [
    "angular",
    "components",
    "material",
    "ui",
    "ui-kit",
    "uipath"
  ],
  "maintainers": [
    "adrian-macuc",
    "anbalase",
    "eugen-v",
    "sergiubologa",
    "toxik",
    "vasyop",
    "vladjerca"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:theme": "scss-bundle -c scss-bundle.config.json",
    "build:readme": "cpx ./README.md ./dist/angular",
    "build:prod": "npm-run-all lint build build:theme build:readme",
    "test:watch": "ng test",
    "test:headless": "ng test --watch=false --browsers ChromeHeadless",
    "test:coverage": "ng test --code-coverage --watch=false --browsers ChromeHeadless",
    "lint": "ng lint",
    "lint:fix": "tslint --fix projects/**/*.ts",
    "e2e": "ng e2e",
    "package": "npm-run-all build:prod",
    "docs": "compodoc",
    "commit": "git-cz",
    "check:exports": "ts-node --project ./tsconfig.node.json ./.build/check-public-exports",
    "check:commits": "node ./.build/commitlint",
    "publish:npm": "json -I -f dist/angular/package.json -e 'delete this.publishConfig' && npm publish dist/angular",
    "publish:gh": "json -I -f dist/angular/package.json -e 'this.publishConfig = {registry:\"https://npm.pkg.github.com/\"}' && npm publish dist/angular"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/prompt"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "8.1.1",
    "@angular/cdk": "8.1.1",
    "@angular/cdk-experimental": "8.1.1",
    "@angular/common": "8.1.1",
    "@angular/compiler": "8.1.1",
    "@angular/core": "8.1.1",
    "@angular/forms": "8.1.1",
    "@angular/material": "^8.1.1",
    "@angular/platform-browser": "8.1.1",
    "@angular/platform-browser-dynamic": "8.1.1",
    "@angular/router": "8.1.1",
    "clipboard": "2.0.4",
    "core-js": "2.5.4",
    "lodash-es": "4.17.14",
    "moment": "2.24.0",
    "moment-timezone": "0.5.25",
    "object-hash": "1.3.1",
    "rxjs": "6.5.2",
    "scroll-into-view-if-needed": "2.2.20",
    "tslib": "1.9.3",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.1",
    "@angular-devkit/build-ng-packagr": "~0.801.1",
    "@angular/cli": "8.1.1",
    "@angular/compiler-cli": "8.1.1",
    "@angular/language-service": "8.1.1",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@commitlint/core": "^8.1.0",
    "@commitlint/prompt": "^8.1.0",
    "@compodoc/compodoc": "^1.1.10",
    "@octokit/rest": "^16.28.5",
    "@types/chalk": "^2.2.0",
    "@types/clipboard": "2.0.1",
    "@types/faker": "4.1.5",
    "@types/jasmine": "3.3.12",
    "@types/jasminewd2": "2.0.6",
    "@types/lodash-es": "4.17.3",
    "@types/moment-timezone": "0.5.12",
    "@types/node": "12.0.2",
    "@types/object-hash": "1.2.0",
    "chalk": "^2.4.2",
    "codelyzer": "^5.0.1",
    "commitizen": "^4.0.0",
    "cpx": "1.5.0",
    "faker": "4.1.0",
    "gh-pages": "2.0.1",
    "glob": "^7.1.4",
    "husky": "2.3.0",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "json": "^9.0.6",
    "karma": "4.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.5",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "karma-junit-reporter": "1.2.0",
    "ng-packagr": "5.2.0",
    "npm-run-all": "4.1.5",
    "protractor": "5.4.2",
    "rxjs-tslint": "0.1.7",
    "rxjs-tslint-rules": "4.23.2",
    "scss-bundle": "2.5.1",
    "ts-node": "8.2.0",
    "tsickle": "0.35.0",
    "tslib": "1.9.3",
    "tslint": "5.16.0",
    "tslint-consistent-codestyle": "1.15.1",
    "tslint-jasmine-rules": "1.6.0",
    "typescript": "3.4.5"
  }
}
